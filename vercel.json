{
  "buildCommand": "npm run dashboard:build",
  "outputDirectory": "public",
  "rewrites": [
    {
      "source": "/api/alerts/:id",
      "destination": "/api/alerts/[id]"
    },
    {
      "source": "/api/positions",
      "destination": "/api/positions"
    },
    {
      "source": "/api/trades-log",
      "destination": "/api/trades-log"
    },
    {
      "source": "/((?!api/).*)",
      "destination": "/index.html"
    }
  ]
}
